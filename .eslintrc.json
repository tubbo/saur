{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": ["eslint:recommended"],
  "rules": {
    "no-return-await": "error",
    "require-await": "error",
    "prettier/prettier": "error"
  },
  "globals": {
    "Deno": true,
    "window": true,
    "globalThis": true,
    "App": true,
    "console": true,
    "TextEncoder": true,
    "TextDecoder": true
  }
}
