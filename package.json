{
  "name": "saur",
  "version": "0.0.1",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "globals": {
      "Deno": true,
      "window": true,
      "globalThis": true,
      "console": true
    }
  },
  "prettier": {
    "proseWrap": "always",
    "trailingComma": "all"
  }
}
